{
  "version": 2,
  "name": "nuxt-express-now",
  "github": {
    "autoAlias": false
  },
  "alias": "nuxt-express-now-dev.now.sh",
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder@0.16.4",
      "config": {
        "serverFiles": [
          "./server/**/*",
          "./constants.js"
        ]
      }
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "APP_ENV": "development",
      "API_URL": "http://dev-api.mywebsite.com",
      "BASIC_AUTH": "enabled"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "APP_ENV": "development",
    "API_URL": "http://dev-api.mywebsite.com",
    "BASIC_AUTH": "enabled"
  }
}
