{
  "version": 2,
  "name": "nuxt-express-now",
  "github": {
    "autoAlias": false
  },
  "alias": [
    "nuxt-express-now-staging.now.sh",
    "nuxt-express-now-1-0-5-staging.now.sh"
  ],
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder@0.16.4",
      "config": {
        "serverFiles": [
          "./server/**/*",
          "./constants.js"
        ]
      }
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "APP_ENV": "staging",
      "APP_VERSION": "1.0.5-staging",
      "API_URL": "http://staging-api.mywebsite.com",
      "BASIC_AUTH": "disabled"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "APP_ENV": "staging",
    "APP_VERSION": "1.0.5-staging",
    "API_URL": "http://staging-api.mywebsite.com",
    "BASIC_AUTH": "disabled"
  }
}
